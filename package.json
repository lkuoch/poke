{
  "name": "poke",
  "version": "0.1.1",
  "author": "lkuoch",
  "description": "",
  "homepage": ".",
  "main": "build/electron/main.js",
  "private": true,
  "scripts": {
    "start:web": "set HTTPS=true&&react-scripts start",
    "start:electron": "concurrently \"react-scripts start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "build:web": "react-scripts build",
    "build:electron": "yarn build && tsc -p electron && electron-builder",
    "test": "react-scripts test",
    "postinstall": "electron-builder install-app-deps",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.4",
    "fomantic-ui-css": "^2.8.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "redux-api-middleware": "^3.2.1",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "25.2.1",
    "@types/node": "13.11.1",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@types/redux-api-middleware": "^3.0.7",
    "@types/redux-saga": "^0.10.5",
    "concurrently": "^5.1.0",
    "electron": "^8.2.2",
    "electron-builder": "^22.4.1",
    "electron-devtools-installer": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "node-sass": "^4.13.1",
    "react-scripts": "3.4.1",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "3.8.3",
    "wait-on": "^4.0.2"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
